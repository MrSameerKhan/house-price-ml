pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }

        stage('Sanity Check') {
            steps {
                sh 'python3 --version || true'
            }
        }

        stage('Docker Version') {
            steps {
                sh 'docker --version || true'
            }
        }
    }
}
